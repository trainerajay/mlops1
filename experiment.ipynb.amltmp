    },
    {
      "cell_type": "code",
      "source": [
        "experiment = Experiment(workspace=workspace, name=\"iris-experiment\")\n",
        "\n",
        "dataset = Dataset.get_by_name(workspace, name='Iris')\n",
        "df = dataset.to_pandas_dataframe() #dont forget to edit this line\n",
        "\n",
        "features = ['SepalLengthCm', 'SepalWidthCm', 'PetalLengthCm', 'PetalWidthCm']\n",
        "taget = 'Species'"
      ],
      "outputs": [],
      "execution_count": 5,
      "metadata": {
        "jupyter": {
          "source_hidden": false,
          "outputs_hidden": false
        },
        "nteract": {
          "transient": {
            "deleting": false
          }
        },
        "gather": {
          "logged": 1692632242250
        }
      }
    },
    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.tree import DecisionTreeClassifier\n",
        "X_train, X_test, y_train, y_test = train_test_split(df[features],df[taget] , test_size=0.1, shuffle=True)\n",
        "#step-1: initialise the model class\n",
        "clf = DecisionTreeClassifier(criterion=\"entropy\") #Information gain as criteria\n",
        "#step-2: train the model on training set\n",
        "clf.fit(X_train,y_train)"
      ],
      "outputs": [
